{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api/survey/",
        "/api/direction-zone-entity-type-dangerous-situation",
        "/api/header/",
        "/api/user/",
        "/api/survey/history/"
      ],
      "cacheConfig": {
        "maxAge": "7d",
        "strategy": "freshness"
      }
    }
  ],
  "navigationUrls": [
    "!/api/**",
    "!/admin/**",
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ]
}
